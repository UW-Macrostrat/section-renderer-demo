#!/usr/bin/env zsh

fn=$PROJECT_DIR/versioned/Products/webroot
query_dir="$fn/queries"
mkdir -p $fn $query_dir

rm -rf node_modules/.cache

# Serialize query datasets
./serialize-queries $query_dir

# Compile code
cp app/web-index.html $fn/index.html
webpack $@
## Use --watch flag to start browserSync

