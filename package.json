{
  "name": "@macrostrat/section-renderer",
  "version": "1.0.0",
  "description": "A renderer for Macrostrat columns",
  "scripts": {
    "dev:legacy": "npm-run-all -l -p dev watch:**",
    "postinstall": "lerna bootstrap --hoist",
    "dev": "parcel apps/index.html",
    "deploy:steno": "./deploy/deploy-steno.sh",
    "watch:map": "npm --prefix packages/map-components run watch:main",
    "watch:ui": "npm --prefix packages/ui-components run dev",
    "watch:column": "npm --prefix packages/column-components run watch",
    "watch:timescale": "npm --prefix packages/timescale run watch",
    "build": "parcel build --public-url . apps/index.html",
    "deploy": "./deploy/deploy.sh",
    "clean": "rm -rf .cache dist"
  },
  "author": "Daven Quinn",
  "license": "ISC",
  "alias": {
    "@macrostrat/column-components": "@macrostrat/column-components/src",
    "@macrostrat/map-components": "@macrostrat/map-components/src",
    "@macrostrat/ui-components": "@macrostrat/ui-components/src",
    "common": "./packages/common/src"
  },
  "devDependencies": {
    "@parcel/resolver-glob": "^2.0.0-nightly.2338",
    "@types/d3-geo": "^2.0.0",
    "@types/geojson": "^7946.0.7",
    "bulk": "^2.0.1",
    "husky": "^4.3.5",
    "lerna": "^4.0.0",
    "linklocal": "^2.8.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "stylus": "^0.54.7",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@macrostrat/column-components": "file:./packages/column-components",
    "@macrostrat/corelle": "^1.1.0",
    "@macrostrat/hyper": "^1.2.14",
    "@macrostrat/map-components": "file:./packages/map-components",
    "@macrostrat/timescale": "file:./packages/timescale",
    "@macrostrat/ui-components": "file:./packages/ui-components",
    "@parcel/transformer-image": "^2.0.0-nightly.2400",
    "@parcel/transformer-stylus": "^2.0.0-nightly.778",
    "@react-hook/size": "^2.1.1",
    "@types/react": "^16.14.2",
    "@vx/axis": "^0.0.199",
    "@vx/gradient": "^0.0.199",
    "@vx/scale": "^0.0.199",
    "@vx/shape": "^0.0.199",
    "axios": "^0.21.1",
    "chroma-js": "^2.1.0",
    "core-js": "^3.6.1",
    "d3": "^5.15.0",
    "d3-array": "^2.8.0",
    "d3-geo-voronoi": "^1.6.0",
    "d3-path": "^1.0.9",
    "d3-scale-chromatic": "^1.5.0",
    "d3-selection": "^1.4.1",
    "figment-ui": "^0.3.1",
    "geologic-patterns": "github:davenquinn/geologic-patterns",
    "immutability-helper": "^3.0.1",
    "labella": "^1.1.4",
    "parcel": "^2.0.0-nightly.776",
    "postcss": "^8.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-draggable": "^4.2.0",
    "react-images": "^1.1.0-beta.0",
    "react-json-view": "^1.19.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scroll": "^1.7.15",
    "react-spring": "^8.0.27",
    "react-svg-textures": "^1.4.7",
    "regenerator-runtime": "^0.13.3",
    "supercluster": "^7.1.3",
    "topojson-client": "^3.1.0",
    "ui-box": "^3.0.2",
    "ui-components": "^0.1.5",
    "underscore": "^1.12.0",
    "use-async-effect": "^2.2.1"
  },
  "prettier": {}
}
